<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Cars</title>
  </head>
  <body>
    <%- include('../partials/_navbar.ejs') %>
    <h1>All Cars</h1>
    <div>
      <% if (carsEl.length > 0) { %>
        <ul>
          <% carsEl.forEach(car => { %>
            <li>
              
              <a href="/cars/<%= car._id %>">


                <%= car.name %>
              </a>
            </li>
          <% }); %>
        </ul>
      <% } else { %>
        <p>There are no cars.</p>
      <% } %>
    </div>
    <a href="/cars/new">Add a New Car</a>
  </body>
</html>